{
  "name": "next-reason-boilerplate",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "now": {
    "alias": "now.example.com"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "bin": "index.js",
  "scripts": {
    "now": "now --public && now alias",
    "bsb-watch": "bsb -clean-world -make-world -w",
    "dev": "npm run lint-fix && node server.js",
    "dev-pure": "node server.js",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "lint": "eslint --ext .js pages components apollo",
    "lint-fix": "eslint --fix --ext .js pages components apollo",
    "lint:watch": "esw -w --fix components libraries server",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "dev-concurrently": "concurrently \"bsb -clean-world -make-world -w\" \"node server.js\"",
    "bsb": "node ./node_modules/bs-platform/lib/bsb -clean-world -make-world",
    "bsb-local": "bsb -clean-world -make-world",
    "next-build": "next build",
    "build": "NODE_ENV=production rm -rf .next && npm run bsb && next build",
    "build-local": "NODE_ENV=production rm -rf .next && npm run bsb-local && next build",
    "build-zip": "npm run build && npm run linux-zip",
    "build-repackzip": "npm run build && npm run repack-zip-flow",
    "extra-zip": "zip -qr next-reason-boilerplate.zip lambda.js .next .ebextensions node_modules/bs-platform/lib/js lib/bs",
    "start": "NODE_ENV=production node server.js",
    "start:multicore": "node ./node_modules/pm2/bin/pm2 start server.js -i max --attach --name MyApp",
    "repack-zip-flow": "rm -rf next-reason-boilerplate.zip && npm run repack-zip-alt && npm run extra-zip && npm run zip-clean",
    "repack-zip-alt": "node ./node_modules/repack-zip-alt/bin/repack-zip-alt",
    "prune": "prune --production",
    "permissions": "chmod a+x node_modules/.bin/next",
    "linux-zip": "zip -rq next-reason-boilerplate.zip . -x \"*.zip\" && npm run zip-clean",
    "package": "serverless package --verbose",
    "deploy": "serverless deploy",
    "deploy-function": "serverless deploy function --function next",
    "rollup": "NODE_ENV=production rollup -c",
    "next-pkg": "pkg . --out-dir dist --target node8-linux-x64",
    "modclean": "modclean",
    "zip-clean": "zip -dq next-reason-boilerplate.zip \\*.map node_modules/antd/dist/\\* node_modules/aws-sdk/\\* node_modules/webpack-dev-middleware/\\* node_modules/webpack-hot-middleware/\\* "
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@quentin-sommer/react-useragent": "^2.0.0",
    "@sentry/browser": "^4.1.1",
    "@sentry/node": "^4.1.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.10.1",
    "antd-mobile": "^2.2.5",
    "apminsight": "^1.2.0",
    "apollo-boost": "^0.1.16",
    "babel-plugin-import": "^1.10.0",
    "bs-ant-design-alt": "^1.0.7",
    "bs-ant-design-mobile": "^1.0.2",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "express-healthcheck": "^0.1.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "helmet": "^3.14.0",
    "isomorphic-unfetch": "^3.0.0",
    "less": "^3.8.1",
    "next": "^7.0.2",
    "next-link": "^1.0.3",
    "next-offline": "^3.0.2",
    "next-routes": "^1.4.2",
    "next-rum": "^1.1.0",
    "next-seo": "^1.2.0",
    "pm2": "^3.2.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "reason-apollo": "^0.14.1",
    "reason-react": "^0.5.3",
    "require-hacker": "^3.0.1",
    "shrink-ray-current": "^2.1.2",
    "svg-sprite-loader": "^4.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "babel-eslint": "10.0.1",
    "babel-plugin-bucklescript": "^0.5.2",
    "bs-platform": "^4.0.6",
    "dotenv": "^6.1.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-graphql": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "graphql_ppx": "^0.2.7",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "ngrok": "3.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.14.3",
    "repack-zip-alt": "^0.2.8",
    "rimraf": "2.6.2"
  }
}
