{
  "name": "next-reason-boilerplate",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "bin": "index.js",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 npm run build",
    "now": "now --public && now alias",
    "bsb-watch": "bsb  -make-world -w",
    "bsb-clean-watch": "bsb -clean-world -make-world -w",
    "dev": "npm run lint-fix && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node server.js",
    "dev-pure": "node server.js",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "lint": "eslint --ext .js pages components util server.js",
    "lint-fix": "eslint --fix --ext .js pages components util server.js",
    "lint:watch": "esw -w --fix components libraries server",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "dev-concurrently": "concurrently \"bsb -clean-world -make-world -w\" \"node server.js\"",
    "bsb": "node ./node_modules/bs-platform/lib/bsb -clean-world -make-world",
    "bsb-local": "bsb -clean-world -make-world",
    "next-build": "next build",
    "build": "NODE_ENV=production rm -rf .next && npm run bsb && next build",
    "build-local": "NODE_ENV=production rm -rf .next && npm run bsb-local && next build",
    "build-zip": "npm run build && npm run linux-zip",
    "build-repackzip": "npm run build && npm run repack-zip-flow",
    "extra-zip": "zip -qr next-reason-boilerplate.zip lambda.js .next .ebextensions node_modules/bs-platform/lib/js lib/bs",
    "start-dev": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=production LOCAL_ENV=true node server.js",
    "start": "NODE_ENV=production node server.js",
    "start:multicore": "node ./node_modules/pm2/bin/pm2 start server.js -i max --attach --name MyApp",
    "repack-zip-flow": "rm -rf next-reason-boilerplate.zip && npm run repack-zip-alt && npm run extra-zip && npm run zip-clean",
    "repack-zip-alt": "node ./node_modules/repack-zip-alt/bin/repack-zip-alt",
    "prune": "prune --production",
    "permissions": "chmod a+x node_modules/.bin/next",
    "linux-zip": "zip -rq next-reason-boilerplate.zip . -x \"*.zip\" && npm run zip-clean",
    "package": "serverless package --verbose",
    "deploy": "serverless deploy",
    "deploy-function": "serverless deploy function --function next",
    "rollup": "NODE_ENV=production rollup -c",
    "next-pkg": "pkg . --out-dir dist --target node8-linux-x64",
    "modclean": "modclean",
    "zip-clean": "zip -dq next-reason-boilerplate.zip \\*.map node_modules/antd/dist/\\* node_modules/aws-sdk/\\* node_modules/webpack-dev-middleware/\\* node_modules/webpack-hot-middleware/\\* ",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "now && now alias && now rm next-reason-boilerplate --safe --yes",
    "test": "bsb -make-world && jest"
  },
  "dependencies": {
    "@4c/react-intl": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@quentin-sommer/react-useragent": "^2.0.0",
    "@sentry/browser": "^5.2.0",
    "@sentry/node": "^5.2.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "accepts": "^1.3.7",
    "antd": "3.17.0",
    "antd-mobile": "^2.2.13",
    "apminsight": "^1.4.2",
    "apollo-boost": "^0.3.1",
    "apollo-cache-hermes": "^0.8.9",
    "apollo-link-context": "^1.0.17",
    "apollo-link-retry": "^2.2.13",
    "apollo-upload-client": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "bs-ant-design-mobile": "^1.0.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-healthcheck": "^0.1.0",
    "glob": "^7.1.4",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.18.0",
    "hoist-non-react-statics": "^3.3.0",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "less": "^3.9.0",
    "next": "8.1.0",
    "next-offline": "^3.4.0",
    "next-preload-headers": "^3.0.4",
    "next-rum": "^1.1.0",
    "next-seo": "^1.11.2",
    "next-transpile-modules": "^2.3.1",
    "nib": "^1.1.2",
    "nookies": "^2.0.5",
    "pm2": "^3.5.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-iframe": "^1.8.0",
    "react-intl": "^2.8.0",
    "reason-apollo": "file:reason-apollo-0.15.2.tgz",
    "reason-react": "^0.7.0",
    "recompose": "^0.30.0",
    "require-hacker": "^3.0.1",
    "shrink-ray-current": "^4.0.0",
    "stylus": "^0.54.5",
    "svg-sprite-loader": "^4.1.6",
    "tamper": "^1.1.0",
    "throttle-debounce": "^2.1.0",
    "upgrade-reason-react": "git+https://github.com/chenglou/upgrade-reason-react.git#0.6.0-to-0.7.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@zeit/next-css": "1.0.2-canary.2",
    "@zeit/next-less": "1.0.2-canary.2",
    "@zeit/next-stylus": "1.0.2-canary.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-bucklescript": "^0.5.3",
    "bs-ant-design-alt": "file:../bs-ant-design/bs-ant-design-alt-2.0.0-alpha.6.tgz",
    "bs-css": "^8.0.4",
    "bs-fontawesome": "^1.0.0",
    "bs-next-seo": "^1.0.1",
    "bs-platform": "^5.0.4",
    "bs-react-iframe": "^1.0.0",
    "bs-react-intl": "0.9.0-beta.2",
    "cross-env": "^5.2.0",
    "devcert": "^1.0.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-watch": "^5.1.2",
    "graphql_ppx": "^0.2.8",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.6",
    "next-purgecss": "^3.0.1",
    "ngrok": "^3.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "react-addons-test-utils": "^15.6.2",
    "repack-zip-alt": "^0.2.9",
    "rimraf": "^2.6.3",
    "targets-webpack-plugin": "^3.0.0",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
