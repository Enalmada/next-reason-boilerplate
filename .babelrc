{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "babel-plugin-bucklescript",
    [
      "@babel/plugin-proposal-decorators",
      {
        "decoratorsBeforeExport": true
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      },
      "antd"
    ],
    [
      "import",
      {
        "libraryName": "ant-mobile",
        "style": true
      },
      "ant-mobile"
    ]
  ]
}
